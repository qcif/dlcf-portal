<div class="row">
    <div class="col-md-offset-5 col-md-3">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">AAF Credentials</h3>
        </div>
        <div class="panel-body">
          <% if (sails.config.auth[branding])  { %>
            <a href='<%=sails.config.auth[branding].aaf.loginUrl%>'><img src='/images/aaf_service_223x54.png'/></a>
          <% } else { %>
            <a href='<%=sails.config.auth[sails.config.auth.defaultBrand].aaf.loginUrl%>'><img src='/images/aaf_service_223x54.png'/></a>
          <% } %>
        </div>
      </div>
    </div>
</div>
